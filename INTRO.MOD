PROCEDURE FillAtmosFear (TenNos : ARRAY OF CHAR; AtmosX : CARDINAL);
VAR
  AtmosY, Colour : CARDINAL;
BEGIN
  FOR AtmosY := 133 TO 124 BY -1 DO
     CASE (TenNos[133-AtmosY]) OF
        | '0' : Colour := Black;
        | '1' : Colour := White;
     END;
     Plot (AtmosX, AtmosY, Colour);
     Lib.Delay (5);
  END;
END FillAtmosFear;

